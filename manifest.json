{
  "name": "Readable Wikipedia",
  "short_name": "ReadableWiki",
  "version": "1.4.3",
  "description": "Make Wikipedia more readable while keeping all of the functionality. Fix excessively long lines and make the font size adjustable.",
  "content_scripts": [
    {
      "matches": ["http://*.wikipedia.org/*", "https://*.wikipedia.org/*"],
      "css": ["wiki.css"],
      "js": ["zepto.min.js", "wiki.js"]
    }
  ],
  "web_accessible_resources": [
   "DroidSerif-Regular-webfont.woff",
   "DroidSerif-Bold-webfont.woff",
   "DroidSerif-Italic-webfont.woff",
   "DroidSerif-BoldItalic-webfont.woff"
   ],
  "icons": { "16": "16.png",
          "48": "48.png",
          "128": "128.png" },
  "manifest_version": 2
}
